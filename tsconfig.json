{
  "compilerOptions": {
    "strictNullChecks": true,
    // "module": "es6",
    "jsx": "react",
    "target": "es6",
    // "allowJs": true,

    "sourceMap": false,
    "pretty": true,

    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
  },

  "exclude": [
    "node_modules",
  ]
}
